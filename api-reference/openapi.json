    "/api/scrap/hotel": {
      "post": {
        "summary": "Scrape hotel information",
        "responses": {
          "200": {
            "description": "Successful response"
          }
        }
      }
    },
    "/api/search": {
      "get": {
        "summary": "Search for travel options",
        "responses": {
          "200": {
            "description": "Successful response"
          }
