{
  "openapi": "3.0.0",
  "info": {
    "title": "Wanderlust API",
    "version": "1.0.0",
    "description": "API for the Wanderlust travel application"
  },
  "servers": [
    {
      "url": "https://api.wanderlust.com/v1"
    }
  ],
  "paths": {
